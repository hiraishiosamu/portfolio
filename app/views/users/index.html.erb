<% provide(:title, 'All users') %>
<h1>全てのユーザー</h1>

<%= will_paginate %>

<!--#検索機能の追加-->
<p>ユーザー検索</p>
<%= form_tag(users_path, method: :get) do %>
  <%= text_field_tag :search %>
  <%= submit_tag '検索', :name => nil %>
<% end %>
  
<ul class="users">
  <%= render @users %>
</ul>


<%= will_paginate %>